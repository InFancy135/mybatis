<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.MemberMapper">
<select id="findMemberById" resultType="vo.Member">
	select
		id,
		name,
		gender,
		birth,
		email
	from
		member
	where
		id= #[id]
</select>
  
</mapper>